<h1>Products#index</h1>
<p>Find me in app/views/products/index.html.erb</p>
<% @products.each do |product| %>
  <div class="title">
    <%= product.title %>
  </div>

  <div class="body">
    <%= product.body %>
  </div>

  <div class="location">
    <%= product.location %>
  </div>

  <div class="category">
    <%= product.category %>
  </div>

  <div class="price">
    <%= product.price %>
  </div>

  <div class="quantity">
    <%= product.quantity %>
  </div>

<% end %>
